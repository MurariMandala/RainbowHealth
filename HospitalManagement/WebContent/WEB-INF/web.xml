<?xml version="1.0" encoding="UTF-8"?>
<web-app>

 <servlet>
    <servlet-name>UserLoginServlet</servlet-name>
    <servlet-class>com.project.servlet.UserLoginServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>AddReportServlet</servlet-name>
    <servlet-class>com.project.servlet.AddReportServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>TodaysReportServlet</servlet-name>
    <servlet-class>com.project.servlet.TodaysReportServlet</servlet-class>
  </servlet>
  
   <servlet-mapping>
    <servlet-name>UserLoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>AddReportServlet</servlet-name>
    <url-pattern>/addReport</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TodaysReportServlet</servlet-name>
    <url-pattern>/totalBills</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>